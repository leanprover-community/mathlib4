@[simps]
def pageFunctor (r : ‚Ñ§) [‚àÄ (E : SpectralSequence C c r‚ÇÄ), E.HasPage r] :
    SpectralSequence C c r‚ÇÄ ‚•§ HomologicalComplex C (c r) where
  obj E := E.page r
