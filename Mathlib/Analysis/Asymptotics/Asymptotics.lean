    f'' =O[cofinite] g'' ‚Üî ‚àÉ C, ‚àÄ x, ‚Äñf'' x‚Äñ ‚â§ C * ‚Äñg'' x‚Äñ :=
  ‚ü®fun h' =>
    let ‚ü®C, _C‚ÇÄ, hC‚ü© := bound_of_isBigO_cofinite h'
    ‚ü®C, fun x => if hx : g'' x = 0 then by simp [h _ hx, hx] else hC hx‚ü©,
    fun h => (isBigO_top.2 h).mono le_top‚ü©
