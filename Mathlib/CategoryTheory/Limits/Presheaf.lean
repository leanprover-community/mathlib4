  dsimp only [coconeApp]
  rw [Equiv.apply_symm_apply, ‚Üê uliftYonedaEquiv_naturality, Equiv.apply_symm_apply]
  simp only [‚Üê eq‚ÇÅ, ‚Üê eq‚ÇÇ, ‚Üê eq‚ÇÉ, ‚Üê eq‚ÇÑ, op_unop, Functor.comp_obj,
