    {f : Œ± ‚Üí Œ≤} (f_closed : IsClosedMap f) (f_cont : Continuous f) (F : Filter Œ±) :
    (map f F).lift' closure = map f (F.lift' closure) := by
  rw [map_lift'_eq2 (monotone_closure Œ≤), map_lift'_eq (monotone_closure Œ±)]
  congr
