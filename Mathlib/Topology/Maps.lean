    {f : Œ± ‚Üí Œ≤} (f_closed : IsClosedMap f) (f_cont : Continuous f) (s : Set Œ±) :
    closure (f '' s) = f '' closure s :=
  subset_antisymm (f_closed.closure_image_subset s) (image_closure_subset_closure_image f_cont)

