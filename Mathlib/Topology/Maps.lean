theorem openEmbedding_of_embedding_open {f : Œ± ‚Üí Œ≤} (h‚ÇÅ : Embedding f) (h‚ÇÇ : IsOpenMap f) :
    OpenEmbedding f :=
  ‚ü®h‚ÇÅ, h‚ÇÇ.isOpen_range‚ü©
